<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:janus="http://itrdc.ctg.com/schema/janus"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
    http://itrdc.ctg.com/schema/janus http://itrdc.ctg.com/schema/janus/janus.xsd
    ">
     
    <!-- 当前应用信息配置 -->
    <janus:application name="demo-provider" />
    
    <!-- 连接注册中心配置 -->
    <janus:registry dsn="zk_001"/>
    
    <janus:protocol name="janus" port="20883" />
    
    <!-- 暴露服务配置 -->
    <janus:service interface="com.ctg.itrdc.janus.config.spring.api.DemoService" ref="demoService" delay="300" />
     
    <bean id="demoService" class="com.ctg.itrdc.janus.config.spring.impl.DemoServiceImpl" />
 
</beans>